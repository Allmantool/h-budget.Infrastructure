{
  "name": "h-budget",
  "version": "0.50",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch --open",
    "build": "ng build",
    "build-prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "test-headless-ci-only": "ng test --browsers ChromiumNoSandbox",
    "lint": "ng lint --fix",
    "lint-css": "stylelint src/**/*.css",
    "prettier": "prettier --write src/**/*.{js,ts,css}",
    "eslint": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "sonar-scanner": "sonar-scanner",
    "test-headless": "ng test --watch=false --code-coverage --source-map=false --browsers ChromiumNoSandbox"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged && ng lint --fix && ng test --watch=false --browsers=ChromeHeadless",
      "pre-push": "ng build --aot true && ng lint && ng test --browsers ChromiumNoSandbox"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.5",
    "@angular/cdk": "^16.1.5",
    "@angular/common": "^16.1.5",
    "@angular/compiler": "^16.1.5",
    "@angular/core": "^16.1.5",
    "@angular/forms": "^16.1.5",
    "@angular/material": "^16.1.5",
    "@angular/platform-browser": "^16.1.5",
    "@angular/platform-browser-dynamic": "^16.1.5",
    "@angular/router": "^16.1.5",
    "@ngxs/logger-plugin": "^3.8.1",
    "@ngxs/store": "^3.8.1",
    "apexcharts": "^3.41.0",
    "codelyzer": "^6.0.2",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "material-design-icons": "^3.0.1",
    "ng-apexcharts": "^1.8.0",
    "rxjs": "~7.8.1",
    "stylelint": "^15.10.2",
    "tslib": "^2.6.0",
    "tslint": "^6.1.3",
    "typescript-guid": "^1.0.3",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "^16.1.4",
    "@angular/compiler-cli": "^16.1.5",
    "@ngxs/devtools-plugin": "^3.8.1",
    "@types/jasmine": "~4.3.5",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jasmine-core": "~5.0.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-sonarqube-reporter": "^1.4.0",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "sonarqube-scanner": "^3.0.1",
    "stylelint-prettier": "^4.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.1.6"
  }
}
